{
  "compilerOptions": {
    "target": "es2015",
    "module": "esnext",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "common/*": [
        "src/common/*"
      ],
      "pages/*": [
        "src/pages/*"
      ]
    }
  },
  "include": [
    "src/**/*.vue",
    "src/**/*.js"
  ]
}
